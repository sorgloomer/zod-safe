{
  "compilerOptions": {
    "declaration": true,
    "outDir": "./lib",
    "module": "commonjs",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strict": true,
    "resolveJsonModule": true,
    "importHelpers": true,
    "target": "es5",
    "types": [
      "node"
    ],
    "baseUrl": ".",
    "experimentalDecorators": true
  },
  "moduleResolution": "node"
}
